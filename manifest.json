{
  "manifest_version": 3,
  "name": "HTML table exporter",
  "version": "1.1.0",
  "description": "Allow export table data in web pages to CSV, JSON and Markdown.",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "author": "Lawrence Lagerlof",
  "homepage_url": "https://github.com/llagerlof/htmltableexporter"
}
