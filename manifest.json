{
  "manifest_version": 3,
  "name": "Table to CSV",
  "version": "1.0",
  "description": "Adds a button to tables to copy them as CSV.",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}